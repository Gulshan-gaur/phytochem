<link rel="stylesheet" href="resultstyle.css">
<%- include("partials/header"); -%>

<section id="res">
<div class="resu">
    <h1> Result</h1>
  <table class="table table-striped table-bordered table-hover">
   <tbody>
   <% molwg.find(function(item){ %>
     <tr>
       <td>Molecular_Formula</td>
       <td><%=item.Molecular_formula %></td>
     </tr>
<%  }); %>
</table>
<% if (empty) { %>
  <p>No results found</p>
  <% } %>
</div>
</section>

<div class="ref">
  <% for(let i=1;i<=totalPages ;i++) { %>
      <a href="?mol_wig=<%= wg %>&lessthan=<%= less %>&h_donor=<%= h_donor %>&h_acceptor=<%= h_acceptor %>&aro_ring=<%= aro_ring %>&page=<%= i %>"><%= i %></a>
<%  } %> <a href="/download">.....Download database. </a>
</div>

